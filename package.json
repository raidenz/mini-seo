{
  "name": "mini-seo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "express-http-proxy": "^1.5.1",
    "fetch-everywhere": "^1.0.5",
    "handlebars": "^4.1.2",
    "morgan": "^1.9.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "server": "nodemon --exec babel-node src/server.js --extensions .js",
    "server:clean": "rm -rf server",
    "server:build": "yarn build && npm run server:clean && babel src --out-dir server --copy-files --extensions .js",
    "server:prod": "node server/server.js",
    "server:dummy": "json-server --port 3004 --nc server.json --routes routes.json",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/plugin-transform-typescript": "^7.4.5",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-transform-assets": "^1.0.2",
    "cors": "^2.8.5",
    "json-server": "^0.15.0",
    "lodash": "^4.17.21",
    "nodemon": "^1.19.1"
  },
  "proxy": "http://localhost:3004"
}
